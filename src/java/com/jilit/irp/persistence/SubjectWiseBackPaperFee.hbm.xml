<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class name="com.jilit.irp.persistence.dto.SubjectWiseBackPaperFee" table="SUBJECTWISEBACKPAPERFEE">
    <composite-id class="com.jilit.irp.persistence.dto.SubjectWiseBackPaperFeeId" name="id">
      <key-property name="instituteid" type="string">
        <column length="20" name="INSTITUTEID"/>
      </key-property>
      <key-property name="registrationid" type="string">
        <column length="8" name="REGISTRATIONID"/>
      </key-property>
      <key-property name="subjectid" type="string">
        <column length="20" name="SUBJECTID"/>
      </key-property>
    </composite-id>
    <property name="feeamount" type="integer">
      <column length="8" name="FEEAMOUNT" not-null="true"/>
    </property>
    </class>
</hibernate-mapping>
