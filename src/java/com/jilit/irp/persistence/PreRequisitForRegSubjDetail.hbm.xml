<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jilit.irp.persistence.dto.PreRequisitForRegSubjDetail" table="PREREQUISITFORREGSUBJDETAIL">
        <composite-id class="com.jilit.irp.persistence.dto.PreRequisitForRegSubjDetailId" name="id">
            <key-property name="instituteid" type="string">
                <column length="20" name="INSTITUTEID"/>
            </key-property>
            <key-property name="academicyear" type="string">
                <column length="20" name="ACADEMICYEAR"/>
            </key-property>
            <key-property name="programid" type="string">
                <column length="20" name="PROGRAMID"/>
            </key-property>
            <key-property name="branchid" type="string">
                <column length="20" name="BRANCHID"/>
            </key-property>
            <key-property name="stynumber" type="string">
                <column length="20" name="STYNUMBER"/>
            </key-property>
            <key-property name="subjectid" type="string">
                <column length="20" name="SUBJECTID"/>
            </key-property>
        </composite-id>
        <property name="deactive" type="string">
            <column length="1" name="DEACTIVE"/>
        </property>
        <many-to-one class="com.jilit.irp.persistence.dto.SubjectMaster" fetch="select" insert="false" lazy="false" name="subjectmaster" update="false">
            <column length="20" name="INSTITUTEID" not-null="true"/>
            <column length="20" name="SUBJECTID" not-null="true"/>
        </many-to-one>
        <many-to-one class="com.jilit.irp.persistence.dto.PreRequisitForRegistration" fetch="select" insert="false" lazy="false" name="prerequisitforregistration" update="false">
            <column length="20" name="INSTITUTEID" not-null="true"/>
            <column length="20" name="ACADEMICYEAR" not-null="true"/>
            <column length="20" name="PROGRAMID" not-null="true"/>
            <column length="20" name="BRANCHID" not-null="true"/>
            <column length="20" name="STYNUMBER" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
