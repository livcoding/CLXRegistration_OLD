  <query name="findAllBranchMasterData_StudentRegistration">

         select a
             from BranchMaster a
                       where a.id.instituteid = ?
                            and a.id.programid = ?
                            and coalesce(a.deactive,'N') = 'N'
                            and exists(
                                      select b.studentid
                                             from StudentMaster b, StudentRegistration c
                                                  where b.instituteid = a.id.instituteid
                                                    and b.programid = a.id.programid
                                                    and b.instituteid = c.id.instituteid
                                                    and b.studentid = c.id.studentid
                                                    and b.acadyear = ?
                                                    and c.id.registrationid = ?
                                       )
                              order by a.branchcode desc

</query>
