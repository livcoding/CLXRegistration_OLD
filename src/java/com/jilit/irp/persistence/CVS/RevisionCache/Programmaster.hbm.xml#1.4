  <query name="findProgramCodeRegistrationWise">
    select p from ProgramMaster p
    where p.id.instituteid=?
    and coalesce(p.deactive,'N')='N'
    and p.id.programid in (select pst.programid from ProgramSubjectTagging pst where pst.id.instituteid=? and pst.id.registrationid=? and coalesce(pst.deactive,'N')='N')
    </query>
  <query name="findAllSemester">

       select a.stynumber
       from ProgramSubjectTagging a
       where a.id.instituteid = ?
         and a.id.registrationid = ?
         and a.programid = ?
         and coalesce(a.deactive,'N') = 'N'
         group by a.stynumber
         order by a.stynumber

    </query>
  <query name="findAllProgramMasterData_StudentRegistration">

         select a
             from ProgramMaster a
                       where a.id.instituteid = ?
                            and coalesce(a.deactive,'N') = 'N'
                            and exists(
                                      select b.programid
                                             from StudentMaster b, StudentRegistration c
                                                  where b.instituteid = a.id.instituteid
                                                    and b.programid = a.id.programid
                                                    and b.instituteid = c.id.instituteid
                                                    and b.studentid = c.id.studentid
                                                    and b.acadyear = ?
                                                    and c.id.registrationid = ?
                                       )
                              order by a.programcode desc

    </query>
  <query name="findProgramCode_PST">

        select p from ProgramMaster p
            where p.id.instituteid=?
                  and coalesce(p.deactive,'N')='N'
                  and p.id.programid in
                                       (
                                         select pst.programid
                                               from ProgramSubjectTagging pst
                                                     where pst.id.instituteid=?
                                                       and pst.academicyear = ?
                                                   and pst.id.registrationid=?
                                                       and coalesce(pst.deactive,'N')='N'
                                       )
                                       order by p.programcode



    </query>
