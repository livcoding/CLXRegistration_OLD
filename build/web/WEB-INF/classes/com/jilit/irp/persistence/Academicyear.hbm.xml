  <query name="getAllAcademicYearData">

       select a from Academicyear a
        where a.id.instituteid = ?
           and coalesce(a.deactive,'N') = 'N'
           order by a.id.academicyear desc


    </query>
  <query name="getAllAcademicYearData_StudentRegistration">

      select a from Academicyear a
           where a.id.instituteid = ?
                and coalesce(a.deactive,'N') = 'N'
                and exists
                          (
                            select b.academicyear from StudentMaster b , StudentRegistration c
                                  where b.instituteid = a.id.instituteid
                                    and b.instituteid = c.id.instituteid
                                    and b.studentid = c.id.studentid
                                    and c.id.registrationid = ?
                          )
                   order by a.id.academicyear desc

    </query>
