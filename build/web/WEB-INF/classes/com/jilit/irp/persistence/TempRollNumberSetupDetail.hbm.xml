<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jilit.irp.persistence.dto.TempRollNumberSetupDetail" table="TEMPROLLNUMBERSETUPDETAIL">
        <comment>Enrollment No Generation Table</comment>
        <composite-id class="com.jilit.irp.persistence.dto.TempRollNumberSetupDetailId" name="id">
            <key-property name="instituteid" type="string">
                <column length="20" name="INSTITUTEID"/>
            </key-property>
            <key-property name="groupid" type="string">
                <column length="20" name="GROUPID"/>
            </key-property>
            <key-property name="academicyear" type="string">
                <column length="8" name="ACADEMICYEAR"/>
            </key-property>
            <key-property name="programtypeid" type="string">
                <column length="20" name="PROGRAMTYPEID"/>
            </key-property>
            <key-property name="programid" type="string">
                <column length="20" name="PROGRAMID"/>
            </key-property>
            <key-property name="branchid" type="string">
                <column length="20" name="BRANCHID"/>
            </key-property>
        </composite-id>
        <many-to-one class="com.jilit.irp.persistence.dto.TempRollNumberSetup" fetch="select" insert="false" lazy="false" name="temprollnumbersetup" update="false">
            <column length="20" name="INSTITUTEID" not-null="true"/>
            <column length="20" name="GROUPID" not-null="true"/>
        </many-to-one>
<!--        <many-to-one class="com.jilit.irp.persistence.dto.ProgramType" fetch="select" insert="false" lazy="false" name="programtype" update="false">
            <column length="20" name="INSTITUTEID" not-null="true"/>
            <column length="20" name="PROGRAMTYPEID" not-null="true"/>
        </many-to-one>-->
        <many-to-one class="com.jilit.irp.persistence.dto.BranchMaster" fetch="select" insert="false" lazy="false" name="branchmaster" update="false">
            <column length="20" name="INSTITUTEID" not-null="true"/>
            <column length="20" name="PROGRAMID" not-null="true"/>
            <column length="20" name="BRANCHID" not-null="true"/>
        </many-to-one>
<!--        <many-to-one class="com.jilit.irp.persistence.dto.Academicyear" fetch="select" insert="false" lazy="false" name="academicyear" update="false">
            <column length="20" name="INSTITUTEID" not-null="true"/>
            <column length="8" name="ACADEMICYEAR" not-null="true"/>
        </many-to-one>
        <many-to-one class="com.jilit.irp.persistence.dto.ProgramMaster" fetch="select" insert="false" lazy="false" name="programmaster" update="false">
            <column length="20" name="INSTITUTEID" not-null="true"/>
            <column length="20" name="PROGRAMID" not-null="true"/>
        </many-to-one>-->
    </class>
</hibernate-mapping>

