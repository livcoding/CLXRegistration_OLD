<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


<class name="com.jilit.irp.persistence.dto.CourseCreditWiseFee" table="COURSECREDITWISEFEE">
    <composite-id class="com.jilit.irp.persistence.dto.CourseCreditWiseFeeId" name="id">
      <key-property name="instituteid" type="string">
        <column length="20" name="INSTITUTEID"/>
      </key-property>
      <key-property name="slno" type="short">
        <column length="3" name="SLNO"/>
      </key-property>
      <key-property name="stytypeid" type="string">
        <column length="20" name="STYTYPEID"/>
      </key-property>
    </composite-id>
    <property name="creditfrom" type="double">
      <column length="6" name="CREDITFROM" not-null="true"/>
    </property>
    <property name="creditto" type="double">
      <column length="6" name="CREDITTO" not-null="true"/>
    </property>
    <property name="feeamount" type="integer">
      <column length="8" name="FEEAMOUNT" not-null="true"/>
    </property>
    </class>
</hibernate-mapping>
